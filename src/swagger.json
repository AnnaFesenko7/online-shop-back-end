      "PostProductRequest": {
        "type": "object",
        "required": ["name", "brand", "price"],
        "properties": {
          "name": {
            "type": "string",
            "description": "Product's name",
            "example": "Lipstick"
          },
          "brand": {
            "type": "string",
            "description": "Product's brand",
            "example": "Mary Kay"
          },
          "price": {
            "type": "number",
            "description": "price of product",
            "example": 250
          },
          "rating": {
            "type": "integer",
            "description": "Product's rating",
            "example": 5
          }
        }
      },

      "PostProductResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "succses",
            "example": "Succses"
          },
          "code": {
            "type": "integer",
            "description": "code",
            "example": 200
          },
          "payload": {
            "type": "object",
            "properties": {
              "book": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "Book's title",
                    "example": "The Art of War"
                  },
                  "brand": {
                    "type": "string",
                    "description": "Product's brand",
                    "example": "Sun Tzu"
                  },
                  "price": {
                    "type": "number",
                    "description": "price of product",
                    "example": 250
                  },
                  "rating": {
                    "type": "integer",
                    "description": "Product's rating",
                    "example": 5
                  },

                  "_id": {
                    "type": "string",
                    "description": "id of the book",
                    "example": "633b1aae155b3020faa54d9e"
                  }
                }
              }
            }
          }
        },

        "example": {
          "status": "Success",
          "code": 201,
          "payload": {
            "product": {
              "name": "Lipstick",
              "brand": "Mary Kay",
              "price": 250,
              "rating": 5,
              "_id": "633b1aae155b3020faa54d9e"
            }
          }
        }
      },

      "GetProductsResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "success",
            "example": "Success"
